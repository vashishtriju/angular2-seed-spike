{
  "name": "angular2-seed-spike",
  "version": "1.0.0",
  "description": "Package for Angular 2 Seed Project for Spike or testing.",
  "main": "index.html",
  "scripts": {
    "start": "tsc && concurrently \"tsc -w\" \"lite-server\" \"gulp\" ",
    "tsc": "tsc",
    "gulp-build": "gulp",
    "tsc:w": "tsw -w",
    "lint": "tslint ./app/**/*.ts t verbose",
    "lite": "lite-server",
    "typings": "typings",
    "postinstall": "typings install"
  },
  "keywords": [
    "angular2",
    "spike",
    "angular",
    "seed",
    "angular-seed",
    "spike",
    "solution"
  ],
  "dependencies": {
    "@angular/common": "2.1.2",
    "@angular/compiler": "2.1.2",
    "@angular/core": "2.1.2",
    "@angular/forms": "2.1.2",
    "@angular/http": "2.1.2",
    "@angular/platform-browser": "2.1.2",
    "@angular/platform-browser-dynamic": "2.1.2",
    "@angular/router": "3.1.2",
    "core-js": "2.4.1",
    "reflect-metadata": "0.1.8",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "0.6.26",
    "systemjs": "0.19.40",
    "bootstrap": "3.3.7"
  },
  "devDependencies": {
    "concurrently": "3.1.0",
    "gulp": "^3.9.1",
    "gulp-sass": "^2.3.2",
    "lite-server": "2.2.2",
    "tslint": "3.15.1",
    "typescript": "2.0.7",
    "typings": "1.5.0"
  },
  "author": "Riju Vashisht",
  "license": "MIT"
}
